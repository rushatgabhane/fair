@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix ex: <http://example.org/resource#> .

# Dataset Metadata following FAIR principles
ex:EVDataCatalog a dcat:Catalog ;
    dcterms:title "Electric Vehicle Data Catalog"@en ;
    dcterms:description "A FAIR catalog of electric vehicle population data and related resources"@en ;
    dcterms:publisher ex:Publisher ;
    dcterms:issued "2025-10-26"^^xsd:date ;
    dcterms:modified "2025-10-26"^^xsd:date ;
    dcterms:language <http://id.loc.gov/vocabulary/iso639-1/en> ;
    dcat:dataset ex:ElectricVehicleDataset .

ex:Publisher a foaf:Organization ;
    foaf:name "Example Organization"@en ;
    vcard:hasEmail <mailto:contact@example.org> .

ex:ElectricVehicleDataset a dcat:Dataset ;
    dcterms:identifier "ev-population-2025" ;
    dcterms:title "Electric Vehicle Population Data"@en ;
    dcterms:description "Comprehensive dataset containing electric vehicle registration and population information"@en ;
    dcterms:creator ex:Creator ;
    dcterms:publisher ex:Publisher ;
    dcterms:issued "2025-10-26"^^xsd:date ;
    dcterms:modified "2025-10-26"^^xsd:date ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    dcterms:spatial ex:SpatialCoverage ;
    dcterms:temporal ex:TemporalCoverage ;
    dcat:keyword "electric vehicles"@en, "transportation"@en, "sustainability"@en ;
    dcat:theme <http://eurovoc.europa.eu/4038> ;
    dcat:distribution ex:CSVDistribution, ex:RDFDistribution .

ex:Creator a foaf:Person ;
    foaf:name "Data Curator"@en ;
    vcard:hasEmail <mailto:curator@example.org> .

ex:SpatialCoverage a dcterms:Location ;
    dcterms:identifier "Washington State, USA" ;
    rdfs:label "Washington State"@en .

ex:TemporalCoverage a dcterms:PeriodOfTime ;
    dcat:startDate "2015-01-01"^^xsd:date ;
    dcat:endDate "2025-10-26"^^xsd:date .

ex:CSVDistribution a dcat:Distribution ;
    dcterms:title "Electric Vehicle Data - CSV Format"@en ;
    dcterms:description "Electric vehicle data in CSV format"@en ;
    dcterms:format "text/csv" ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/text/csv> ;
    dcat:accessURL <http://example.org/data/electric_vehicle_population_data.csv> ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> .

ex:RDFDistribution a dcat:Distribution ;
    dcterms:title "Electric Vehicle Data - RDF/Turtle Format"@en ;
    dcterms:description "Electric vehicle data in RDF Turtle format"@en ;
    dcterms:format "text/turtle" ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/text/turtle> ;
    dcat:accessURL <http://example.org/data/data.ttl> ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> .

# FAIR Principles Documentation
ex:FAIRMetadata a rdf:Description ;
    dcterms:title "FAIR Compliance Documentation"@en ;
    rdfs:comment """
    This dataset follows FAIR principles:
    - Findable: Unique identifiers (DOI), rich metadata, indexed in searchable catalogs
    - Accessible: Open access via standard protocols (HTTP), clear license information
    - Interoperable: Uses standard vocabularies (DCAT, Dublin Core), multiple formats
    - Reusable: Detailed provenance, clear licensing (CC-BY-4.0), well-documented structure
    """@en .

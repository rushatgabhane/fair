@prefix dcat:  <http://www.w3.org/ns/dcat#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix prov:  <http://www.w3.org/ns/prov#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix org:   <http://www.w3.org/ns/org#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix :      <https://w3id.org/FAIR-course-UT/2025-2026/group3/> .

#################################################################
# Catalog
#################################################################

:dataCatalog a dcat:Catalog ;
    dct:title "Group 3 FAIR Course Data Catalog" ;
    dct:description "A catalog of FAIRified datasets produced for the FAIR course at the University of Twente (2025–2026)." ;
    dct:creator :group3Team ;
    dcat:dataset :dataSet .

#################################################################
# FAIRified Dataset (derived from original WA EV dataset)
#################################################################

:dataSet a dcat:Dataset ;
    dct:title "Washington State Registered Electric Vehicles (FAIRified)" ;
    dct:description "Dataset of registered electric vehicles in Washington State with semantic annotations following DCAT, QUDT, and GeoSPARQL standards." ;
    dct:source <https://data.wa.gov/api/views/f6w7-q2d2> ;
    prov:wasDerivedFrom <https://data.wa.gov/d/f6w7-q2d2> ;
    dct:creator :group3Team ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:created "2025-10-21"^^xsd:date ;
    dct:issued "2023-10-19"^^xsd:date ;
    dct:modified "2025-09-16"^^xsd:date ;
    dcat:keyword "hybrids", "dol_open_data", "electric", "energy", "environment", "ev",
        "volt", "vehicles", "vehicle", "tesla", "bev", "phev" ;
    dcat:theme <http://eurovoc.europa.eu/100277>, "Transportation" ;
    dcat:landingPage <https://data.wa.gov/d/f6w7-q2d2> ;
    dct:publisher [ a org:Organization ; foaf:name "data.wa.gov" ] ;
    dcat:contactPoint [ a vcard:Kind, vcard:Organization ; vcard:fn "Department of Licensing" ;
                       vcard:hasEmail <mailto:no-reply@data.wa.gov> ] ;
    dcat:distribution :csvDistribution, :rdfDistribution, :jsonDistribution, :xmlDistribution,
                      :ttlDistribution, :ontologyDistribution, :shapesDistribution ;
    dct:conformsTo <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-ontology-2014>,
                   <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-shapes-2014> .

#################################################################
# Distributions
#################################################################

:csvDistribution a dcat:Distribution ;
    dct:title "Original EV dataset (CSV)" ;
    dcat:downloadURL <https://data.wa.gov/api/views/f6w7-q2d2/rows.csv?accessType=DOWNLOAD> ;
    dct:format "text/csv" .

:rdfDistribution a dcat:Distribution ;
    dct:title "Original EV dataset (RDF/XML)" ;
    dcat:downloadURL <https://data.wa.gov/api/views/f6w7-q2d2/rows.rdf?accessType=DOWNLOAD> ;
    dcat:accessURL <https://data.wa.gov/api/views/f6w7-q2d2/columns.rdf> ;
    dct:format "application/rdf+xml" .

:jsonDistribution a dcat:Distribution ;
    dct:title "Original EV dataset (JSON)" ;
    dcat:downloadURL <https://data.wa.gov/api/views/f6w7-q2d2/rows.json?accessType=DOWNLOAD> ;
    dcat:accessURL <https://data.wa.gov/api/views/f6w7-q2d2/columns.json> ;
    dct:format "application/json" .

:xmlDistribution a dcat:Distribution ;
    dct:title "Original EV dataset (XML)" ;
    dcat:downloadURL <https://data.wa.gov/api/views/f6w7-q2d2/rows.xml?accessType=DOWNLOAD> ;
    dcat:accessURL <https://data.wa.gov/api/views/f6w7-q2d2/columns.xml> ;
    dct:format "application/xml" .

:ttlDistribution a dcat:Distribution ;
    dct:title "FAIRified EV dataset (RDF/Turtle)" ;
    dct:description "RDF representation of the FAIRified Washington State EV dataset using the EV ontology." ;
    dct:format "text/turtle" ;
    dcat:accessURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/data> ;
    dcat:downloadURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/data.ttl> .

:ontologyDistribution a dcat:Distribution ;
    dct:title "EV Ontology (2014 subset)" ;
    dct:description "Ontology defining classes and properties used to model the FAIRified Washington EV dataset." ;
    dct:format "text/turtle" ;
    dcat:accessURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-ontology-2014> ;
    dcat:downloadURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-ontology-2014.ttl> .

:shapesDistribution a dcat:Distribution ;
    dct:title "EV SHACL Shapes (2014 subset)" ;
    dct:description "SHACL shapes used to validate the FAIRified Washington EV dataset against the EV ontology." ;
    dct:format "text/turtle" ;
    dcat:accessURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-shapes-2014> ;
    dcat:downloadURL <https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-shapes-2014.ttl> .

#################################################################
# Ontology & SHACL
#################################################################

<https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-ontology-2014>
    a owl:Ontology ;
    dct:title "EV Ontology for Washington State Registered EVs (2014 subset)" ;
    dct:creator :group3Team ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    owl:versionInfo "0.2.0" .

<https://w3id.org/FAIR-course-UT/2025-2026/group3/ev-shapes-2014>
    a sh:NodeShape ;
    dct:title "SHACL Shapes for FAIRified Washington EV Dataset (2014 subset)" ;
    dct:creator :group3Team ;
    dct:description "Validation shapes ensuring that resources using the EV ontology follow the required structure and constraints." .

#################################################################
# Agent
#################################################################

:group3Team a foaf:Agent ;
    foaf:name "FAIR Course Group 3 (2025–2026)" ;
    foaf:mbox <mailto:group3@faircourse.ut.ee> .
